<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta description="Skontaktuj się z Forest Group! Zawsze odpowiemy!">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@200;400;600;700&display=swap" rel="stylesheet">
    <title>Forest Group - Kontakt</title>
    <link rel="stylesheet" href="dist/css/style.min.css">
</head>

<body>
    <!-- @include 'nav.kit' -->
    <main class="main">
        <section class="section contact">
            <div class="container">
                <h2 class="section__heading">kontakt</h2>
                <form class="contact__form">
                    <div class="contact__form-block">
                        <label for="name">Imię:</label>
                        <input type="text" id="name">
                    </div>
                    <div class="contact__form-block">
                        <label for="surname">Nazwisko:</label>
                        <input type="text" id="surname">
                    </div>
                    <div class="contact__form-block">
                        <label for="mail">E-mail:</label>
                        <input type="text" id="mail">
                    </div>
                    <div class="contact__form-block">
                        <label for="content">Treść wiadomości:</label>
                        <textarea id="content"></textarea>
                    </div>
                    <p class="contact__form-error"></p>
                    <button class="contact__form-btn">wyślij</button>
                </form>
            </div>
        </section>
    </main>
    <!-- @include 'footer.kit' -->

    <script src="dist/js/script.min.js"></script>
</body>

</html>